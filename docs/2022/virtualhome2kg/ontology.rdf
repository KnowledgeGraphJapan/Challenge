        <rdfs:range rdf:resource="http://example.org/virtualhome2kg/ontology/Attribute"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/virtualhome2kg/ontology/bbox -->


    <owl:ObjectProperty rdf:about="http://example.org/virtualhome2kg/ontology/bbox">
        <owl:inverseOf rdf:resource="http://example.org/virtualhome2kg/ontology/isBboxOf"/>
        <rdfs:domain rdf:resource="http://example.org/virtualhome2kg/ontology/State"/>
        <rdfs:range rdf:resource="https://www.web3d.org/specifications/X3dOntology4.0#Shape"/>
        <rdfs:comment>3D bounding box</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://example.org/virtualhome2kg/ontology/between -->


    <owl:ObjectProperty rdf:about="http://example.org/virtualhome2kg/ontology/between">
        <rdfs:subPropertyOf rdf:resource="http://example.org/virtualhome2kg/ontology/relatedShape"/>
        <rdfs:domain rdf:resource="https://www.web3d.org/specifications/X3dOntology4.0#Shape"/>
        <rdfs:range rdf:resource="https://www.web3d.org/specifications/X3dOntology4.0#Shape"/>
        <rdfs:comment>used for door object, if door is between kitchen and livingroom, we have edges &lt;door, between, livingroom&gt; and &lt;door, between ,kitchen&gt;</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://example.org/virtualhome2kg/ontology/close -->


    <owl:ObjectProperty rdf:about="http://example.org/virtualhome2kg/ontology/close">
        <rdfs:subPropertyOf rdf:resource="http://example.org/virtualhome2kg/ontology/relatedShape"/>
        <rdfs:domain rdf:resource="https://www.web3d.org/specifications/X3dOntology4.0#Shape"/>
        <rdfs:range rdf:resource="https://www.web3d.org/specifications/X3dOntology4.0#Shape"/>
        <rdfs:comment>edge &lt;object1, close, object2&gt; denotes that the distance between center of object1 (object2) to the bounding box of object2 (object1) is &lt; 1.5 units (~meters)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://example.org/virtualhome2kg/ontology/facing -->


    <owl:ObjectProperty rdf:about="http://example.org/virtualhome2kg/ontology/facing">
        <rdfs:subPropertyOf rdf:resource="http://example.org/virtualhome2kg/ontology/relatedShape"/>
        <rdfs:domain rdf:resource="https://www.web3d.org/specifications/X3dOntology4.0#Shape"/>
        <rdfs:range rdf:resource="https://www.web3d.org/specifications/X3dOntology4.0#Shape"/>
        <rdfs:comment>there is an edge &lt;object1, facing, object2&gt; if object2 is lookable, is visible from object1, and the distance between the centers is &lt; 5 units (~meters). If object1 is a sofa or a chair it should also be turned towards object2</rdfs:comment>
    </owl:ObjectProperty>
    


