    <owl:Class rdf:about="http://www.owl-ontologies.com/VirtualHome.owl#BedTimeSleep">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://example.org/virtualhome2kg/ontology/hasEvent"/>
                        <owl:someValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://example.org/virtualhome2kg/ontology/action"/>
                                <owl:hasValue rdf:resource="http://example.org/virtualhome2kg/ontology/action/sleep"/>
                            </owl:Restriction>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://example.org/virtualhome2kg/ontology/hasEvent"/>
                        <owl:someValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://example.org/virtualhome2kg/ontology/action"/>
                                <owl:hasValue rdf:resource="http://example.org/virtualhome2kg/ontology/action/wakeUp"/>
                            </owl:Restriction>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/VirtualHome.owl#Activity"/>
        <rdfs:comment>Class with sequences for getting ready for the bed, or get ready for 
sleep, or even put the kids to sleep
.</rdfs:comment>
