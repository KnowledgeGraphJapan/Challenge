    <!-- http://example.org/virtualhome2kg/ontology/time -->


    <owl:ObjectProperty rdf:about="http://example.org/virtualhome2kg/ontology/time">
        <rdfs:domain rdf:resource="http://example.org/virtualhome2kg/ontology/Event"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2006/time#TemporalDuration"/>
                    <rdf:Description rdf:about="http://www.w3.org/2006/time#TemporalEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:closeMatch rdf:resource="http://purl.org/NET/c4dm/event.owl#time"/>
    </owl:ObjectProperty>
    


