@prefix : <http://example.org/virtualhome2kg/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.org/virtualhome2kg/ontology/> .

<http://example.org/virtualhome2kg/ontology/> rdf:type owl:Ontology ;
                                               rdfs:comment """This ontology offers vocabularies for representing activities of daily living (ADL) that can be run in the VirtualHome (http://virtual-home.org/), run-time home situations, objects, and relationships among them.
This ontology is an extended version of the HomeOntology (https://github.com/valexande/HomeOntology).""" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#closeMatch
<http://www.w3.org/2004/02/skos/core#closeMatch> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  https://www.web3d.org/specifications/X3dOntology4.0#SFDouble
<https://www.web3d.org/specifications/X3dOntology4.0#SFDouble> rdf:type rdfs:Datatype .


###  https://www.web3d.org/specifications/X3dOntology4.0#SFVec3f
<https://www.web3d.org/specifications/X3dOntology4.0#SFVec3f> rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://example.org/virtualhome2kg/ontology/action
:action rdf:type owl:ObjectProperty ;
        rdfs:domain :Event ;
        rdfs:range :Action .


###  http://example.org/virtualhome2kg/ontology/activity
:activity rdf:type owl:ObjectProperty ;
          rdfs:domain <http://plod.info/rdf/Episode> ;
          rdfs:range <http://www.owl-ontologies.com/VirtualHome.owl#Activity> .


###  http://example.org/virtualhome2kg/ontology/affords
:affords rdf:type owl:ObjectProperty ;
         rdfs:domain :Object ;
         rdfs:range :Action .


###  http://example.org/virtualhome2kg/ontology/agent
:agent rdf:type owl:ObjectProperty ;
       rdfs:domain :Event ;
       rdfs:range :Character ;
       <http://www.w3.org/2004/02/skos/core#closeMatch> <http://purl.org/NET/c4dm/event.owl#agent> .


###  http://example.org/virtualhome2kg/ontology/attribute
:attribute rdf:type owl:ObjectProperty ;
           rdfs:domain :Object ;
           rdfs:range :Attribute .


###  http://example.org/virtualhome2kg/ontology/bbox
:bbox rdf:type owl:ObjectProperty ;
      owl:inverseOf :isBboxOf ;
      rdfs:domain :State ;
      rdfs:range <https://www.web3d.org/specifications/X3dOntology4.0#Shape> ;
      rdfs:comment "3D bounding box" .


###  http://example.org/virtualhome2kg/ontology/between
:between rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :relatedShape ;
         rdfs:domain <https://www.web3d.org/specifications/X3dOntology4.0#Shape> ;
         rdfs:range <https://www.web3d.org/specifications/X3dOntology4.0#Shape> ;
         rdfs:comment "used for door object, if door is between kitchen and livingroom, we have edges <door, between, livingroom> and <door, between ,kitchen>" .


###  http://example.org/virtualhome2kg/ontology/close
:close rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :relatedShape ;
       rdfs:domain <https://www.web3d.org/specifications/X3dOntology4.0#Shape> ;
       rdfs:range <https://www.web3d.org/specifications/X3dOntology4.0#Shape> ;
       rdfs:comment "edge <object1, close, object2> denotes that the distance between center of object1 (object2) to the bounding box of object2 (object1) is < 1.5 units (~meters)" .


###  http://example.org/virtualhome2kg/ontology/facing
:facing rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :relatedShape ;
        rdfs:domain <https://www.web3d.org/specifications/X3dOntology4.0#Shape> ;
        rdfs:range <https://www.web3d.org/specifications/X3dOntology4.0#Shape> ;
        rdfs:comment "there is an edge <object1, facing, object2> if object2 is lookable, is visible from object1, and the distance between the centers is < 5 units (~meters). If object1 is a sofa or a chair it should also be turned towards object2" .


###  http://example.org/virtualhome2kg/ontology/from
:from rdf:type owl:ObjectProperty ;
      rdfs:domain :Event ;
      rdfs:range :Room .


###  http://example.org/virtualhome2kg/ontology/hasActivity
:hasActivity rdf:type owl:ObjectProperty ;
             rdfs:domain <http://plod.info/rdf/Episode> ;
             rdfs:range <http://www.owl-ontologies.com/VirtualHome.owl#Activity> .


###  http://example.org/virtualhome2kg/ontology/hasEvent
:hasEvent rdf:type owl:ObjectProperty ;
          rdfs:domain <http://www.owl-ontologies.com/VirtualHome.owl#Activity> ;
          rdfs:range :Event .


###  http://example.org/virtualhome2kg/ontology/height
:height rdf:type owl:ObjectProperty .


###  http://example.org/virtualhome2kg/ontology/holds_lh
:holds_lh rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :relatedShape ;
          rdfs:domain <https://www.web3d.org/specifications/X3dOntology4.0#Shape> ;
          rdfs:range <https://www.web3d.org/specifications/X3dOntology4.0#Shape> ;
          rdfs:comment "analogue of holds_rh for left hand" .


###  http://example.org/virtualhome2kg/ontology/holds_rh
:holds_rh rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :relatedShape ;
          rdfs:domain <https://www.web3d.org/specifications/X3dOntology4.0#Shape> ;
          rdfs:range <https://www.web3d.org/specifications/X3dOntology4.0#Shape> ;
          rdfs:comment "edge <character, holds_rh, object> is used to indicate that character holds an object in its right hand" .


###  http://example.org/virtualhome2kg/ontology/inside
:inside rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :relatedShape ;
        rdfs:domain <https://www.web3d.org/specifications/X3dOntology4.0#Shape> ;
        rdfs:range <https://www.web3d.org/specifications/X3dOntology4.0#Shape> .


###  http://example.org/virtualhome2kg/ontology/isBboxOf
:isBboxOf rdf:type owl:ObjectProperty ;
          rdfs:domain <https://www.web3d.org/specifications/X3dOntology4.0#Shape> .


###  http://example.org/virtualhome2kg/ontology/isStateOf
:isStateOf rdf:type owl:ObjectProperty ;
           rdfs:domain :State ;
           rdfs:range :Object .


###  http://example.org/virtualhome2kg/ontology/mainObject
:mainObject rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://www.owl-ontologies.com/VirtualHome.owl#object> ;
            rdfs:domain :Event ;
            rdfs:range :Object ;
            rdfs:comment """A main object of an event.
If an agent pours milk into a glass, the value of the mainObject is the milk.""" .


###  http://example.org/virtualhome2kg/ontology/nextActivity
:nextActivity rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :relatedActivity ;
              rdfs:domain <http://www.owl-ontologies.com/VirtualHome.owl#Activity> ;
              rdfs:range <http://www.owl-ontologies.com/VirtualHome.owl#Activity> .


###  http://example.org/virtualhome2kg/ontology/nextEvent
:nextEvent rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :relatedEvent ;
           rdfs:domain :Event ;
           rdfs:range :Event .


###  http://example.org/virtualhome2kg/ontology/nextSituation
:nextSituation rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :relatedSituation ;
               owl:inverseOf :previousSituation ;
               rdfs:domain :Situation ;
               rdfs:range :Situation .


###  http://example.org/virtualhome2kg/ontology/nextState
:nextState rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :relatedState ;
           owl:inverseOf :previousState ;
           rdfs:domain :State ;
           rdfs:range :State .


###  http://example.org/virtualhome2kg/ontology/on
:on rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf :relatedShape ;
    rdfs:domain <https://www.web3d.org/specifications/X3dOntology4.0#Shape> ;
    rdfs:range <https://www.web3d.org/specifications/X3dOntology4.0#Shape> .


###  http://example.org/virtualhome2kg/ontology/partOf
:partOf rdf:type owl:ObjectProperty ;
        rdfs:domain :State ;
        rdfs:range :Situation .


###  http://example.org/virtualhome2kg/ontology/place
:place rdf:type owl:ObjectProperty ;
       rdfs:domain :Event ;
       rdfs:range :Room .


###  http://example.org/virtualhome2kg/ontology/previousActivity
:previousActivity rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :relatedActivity .


###  http://example.org/virtualhome2kg/ontology/previousEvent
:previousEvent rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :relatedEvent ;
               rdfs:domain :Event ;
               rdfs:range :Event .


###  http://example.org/virtualhome2kg/ontology/previousSituation
:previousSituation rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :relatedSituation ;
                   rdfs:domain :Situation ;
                   rdfs:range :Situation .


###  http://example.org/virtualhome2kg/ontology/previousState
:previousState rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :relatedState ;
               rdfs:domain :State ;
               rdfs:range :State .


###  http://example.org/virtualhome2kg/ontology/relatedActivity
:relatedActivity rdf:type owl:ObjectProperty ;
                 rdfs:domain <http://www.owl-ontologies.com/VirtualHome.owl#Activity> ;
                 rdfs:range <http://www.owl-ontologies.com/VirtualHome.owl#Activity> .


###  http://example.org/virtualhome2kg/ontology/relatedEvent
:relatedEvent rdf:type owl:ObjectProperty ;
              rdfs:domain :Event ;
              rdfs:range :Event .


###  http://example.org/virtualhome2kg/ontology/relatedShape
:relatedShape rdf:type owl:ObjectProperty ;
              rdfs:domain <https://www.web3d.org/specifications/X3dOntology4.0#Shape> ;
              rdfs:range <https://www.web3d.org/specifications/X3dOntology4.0#Shape> .


###  http://example.org/virtualhome2kg/ontology/relatedSituation
:relatedSituation rdf:type owl:ObjectProperty ;
                  rdfs:domain :Situation ;
                  rdfs:range :Situation .


###  http://example.org/virtualhome2kg/ontology/relatedState
:relatedState rdf:type owl:ObjectProperty ;
              rdfs:domain :State ;
              rdfs:range :State .


###  http://example.org/virtualhome2kg/ontology/situationAfterEvent
:situationAfterEvent rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :relatedSituation ;
                     rdfs:domain :Event ;
                     rdfs:range :Situation .


###  http://example.org/virtualhome2kg/ontology/situationBeforeEvent
:situationBeforeEvent rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :relatedSituation ;
                      rdfs:domain :Event ;
                      rdfs:range :Situation .


###  http://example.org/virtualhome2kg/ontology/state
:state rdf:type owl:ObjectProperty ;
       rdfs:domain :State ;
       rdfs:range :StateType .


###  http://example.org/virtualhome2kg/ontology/targetObject
:targetObject rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://www.owl-ontologies.com/VirtualHome.owl#object> ;
              rdfs:domain :Event ;
              rdfs:range :Object ;
              rdfs:comment """A target object of an event.
If an agent pours milk into a glass, the value of the value of the targetObject is the glass.""" .


###  http://example.org/virtualhome2kg/ontology/time
:time rdf:type owl:ObjectProperty ;
      rdfs:domain :Event ;
      rdfs:range [ rdf:type owl:Class ;
                   owl:unionOf ( <http://www.w3.org/2006/time#TemporalDuration>
                                 <http://www.w3.org/2006/time#TemporalEntity>
                               )
                 ] ;
      <http://www.w3.org/2004/02/skos/core#closeMatch> <http://purl.org/NET/c4dm/event.owl#time> .


###  http://example.org/virtualhome2kg/ontology/to
:to rdf:type owl:ObjectProperty ;
    rdfs:domain :Event ;
    rdfs:range :Room .


###  http://example.org/virtualhome2kg/ontology/unit
:unit rdf:type owl:ObjectProperty ;
      rdfs:range :Unit .


###  http://example.org/virtualhome2kg/ontology/virtualHome
:virtualHome rdf:type owl:ObjectProperty ;
             rdfs:range :VirtualHome .


###  http://example.org/virtualhome2kg/ontology/weight
:weight rdf:type owl:ObjectProperty .


###  http://www.owl-ontologies.com/VirtualHome.owl#object
<http://www.owl-ontologies.com/VirtualHome.owl#object> rdf:type owl:ObjectProperty ;
                                                       rdfs:domain :Event ;
                                                       rdfs:range :Object .


###  http://www.w3.org/2006/time#hasDuration
<http://www.w3.org/2006/time#hasDuration> rdf:type owl:ObjectProperty ;
                                          rdfs:range <http://www.w3.org/2006/time#Duration> .


###  http://www.w3.org/2006/time#unitType
<http://www.w3.org/2006/time#unitType> rdf:type owl:ObjectProperty ;
                                       rdfs:range <http://www.w3.org/2006/time#TemporalUnit> .


#################################################################
#    Data properties
#################################################################

###  http://example.org/virtualhome2kg/ontology/actionNumber
:actionNumber rdf:type owl:DatatypeProperty ;
              rdfs:domain :Action ;
              rdfs:range xsd:int .


###  http://example.org/virtualhome2kg/ontology/activityNumber
:activityNumber rdf:type owl:DatatypeProperty ;
                rdfs:domain <http://www.owl-ontologies.com/VirtualHome.owl#Activity> ;
                rdfs:range xsd:int .


###  http://example.org/virtualhome2kg/ontology/eventNumber
:eventNumber rdf:type owl:DatatypeProperty ;
             rdfs:domain :Event ;
             rdfs:range xsd:int .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#first
rdf:first rdf:type owl:DatatypeProperty .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#rest
rdf:rest rdf:type owl:DatatypeProperty .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#value
rdf:value rdf:type owl:DatatypeProperty .


###  http://www.w3.org/2006/time#numericDuration
<http://www.w3.org/2006/time#numericDuration> rdf:type owl:DatatypeProperty ;
                                              rdfs:range xsd:decimal .


###  https://www.web3d.org/specifications/X3dOntology4.0#bboxCenter
<https://www.web3d.org/specifications/X3dOntology4.0#bboxCenter> rdf:type owl:DatatypeProperty ;
                                                                 rdfs:domain <https://www.web3d.org/specifications/X3dOntology4.0#Shape> ;
                                                                 rdfs:range <https://www.web3d.org/specifications/X3dOntology4.0#SFVec3f> .


###  https://www.web3d.org/specifications/X3dOntology4.0#bboxSize
<https://www.web3d.org/specifications/X3dOntology4.0#bboxSize> rdf:type owl:DatatypeProperty ;
                                                               rdfs:domain <https://www.web3d.org/specifications/X3dOntology4.0#Shape> ;
                                                               rdfs:range <https://www.web3d.org/specifications/X3dOntology4.0#SFVec3f> .


